#include <stdio.h>
#include <stdint.h>

union Address{
	uint16_t shortAddr;
	uint32_t longAddr;
};



int main(){

	union Address addr;
	addr.shortAddr = 0xABCD;
	addr.longAddr = 0xEEEECCCC;

	printf("%#X", addr.shortAddr);
	printf("%#X", addr.longAddr);








	return 0;
}
